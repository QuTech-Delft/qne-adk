{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "networks": {
      "type": "array",
      "items": {
        "type": "string",
        "readOnly": true,
        "pattern": "^[-a-zA-Z0-9_]+$"
      }
    },
    "roles": {
      "type": "array",
      "patternProperties": {
        "[A-Z][-a-zA-Z0-9_]*": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "networks",
    "roles"
  ]
}
